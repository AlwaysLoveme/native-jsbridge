{
  "name": "native-jsbridge",
  "version": "1.0.2",
  "description": "js plugin for between H5 and Native",
  "main": "lib/index.umd.js",
  "module": "lib/index.esm.js",
  "typings": "lib/typings.d.ts",
  "files": ["lib"],
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint --fix --ext .ts,.tsx,.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy": "cpy ./src/typings.d.ts ./lib",
    "build": "npm run clean && npm run copy && npm run lint && rollup --config config/rollup.config.prod.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlwaysLoveme/native-jsbridge.git"
  },
  "keywords": [
    "rollup",
    "typescript",
    "Native",
    "JSBridge",
    "H5"
  ],
  "author": "zhuxian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlwaysLoveme/native-jsbridge/issues"
  },
  "homepage": "https://github.com/AlwaysLoveme/native-jsbridge#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.3.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "cpy-cli": "^3.1.1",
    "cssnano": "^5.0.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "ssr-window": "^3.0.0"
  }
}
